    </script>
    <!---Load-->
    <script>
        function move() {
            var elem = document.getElementById("myBar");
            var width = 1;
            var id = setInterval(frame, 10);
            $("body").append($("<div></div>").attr("id", "myBar"));
            function frame() {
                if (width >= 100) {
                    clearInterval(id);
                } else {
                    width+=5;
                    elem.style.width = width + '%';
                }
            }
            $(window).load(function() {
                $("#myBar").width("101%").delay(100).fadeOut(400);
            });
        }
    </script>
