@model IEnumerable<WepSiteBanHang.Models.NhaSanXuat>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/LayoutAD/AdinLayout.cshtml";
}



<p>
    @Html.ActionLink("Tạo mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Tên nhà sản xuất")
        </th>
       
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenNSX)
        </td>
       
        
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.MaNSX }) |
